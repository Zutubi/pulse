#helpTag("Build+Artifacts+Tab")
#authorisationTags($principle)
#buildTags($project $result)
<html>
<head>
    <title>#wwtext("name=build") $result.number</title>
</head>
<body>
    <h1>:: #wwtext("name=build") $result.number ::</h1>

    #buildTabs($result "artifacts")

    <h3>#wwtext("name=artifacts.all")</h3>
    <div class="small">
        <a href="#" onclick="expandAllLists('${result.id}_artifacts'); return false">#wwtext("name=expand.all")</a> |
        <a href="#" onclick="collapseAllLists('${result.id}_artifacts'); return false">#wwtext("name=collapse.all")</a>
    </div>
    #buildArtifacts($result)
</html>
