<!DOCTYPE xwork PUBLIC "-//OpenSymphony Group//XWork 1.0//EN"
        "http://www.opensymphony.com/xwork/xwork-1.1.1.dtd">
<xwork>
    <include file="webwork-default-package.xml"/>

    <package name="default" extends="webwork-default-package">
        <default-interceptor-ref name="completeStack"/>

        <action name="default" class="com.cinnamonbob.web.ActionSupport">
            <result name="success" type="velocity">system-starting.vm</result>
        </action>

        <action name="login" class="com.cinnamonbob.web.ActionSupport">
            <result name="success" type="redirect">default.action</result>
        </action>

    </package>

</xwork>
