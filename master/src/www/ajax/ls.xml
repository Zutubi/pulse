<!--
Create an object of the following format:

    object:{
        listing: [
            {
                file:string,
                type:string,
                fid:string
            },....
        ]
    }
-->
<object>
    <pair key="listing">
        <array ref="listing">
            <object>
                <pair key="id">id</pair>
                <pair key="file">name</pair>
                <pair key="type">type</pair>
                <pair key="container">container</pair>
                <pair key="separator">separator</pair>
            </object>
        </array>
    </pair>
</object>
