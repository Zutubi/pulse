<!--
Create an object of the following format:

    object:{
        results: [
            {
                listing: [
                    {
                        file:string,
                        type:string,
                        uid:string
                    },....
                ],
                uid:string,
                displayPath:string
            },....
        ]
    }
-->
<object>
    <pair key="results">
        <array ref="listings">
            <object>
                <pair key="listing">
                    <array ref="listing">
                        <object>
                            <pair key="file">name</pair>
                            <pair key="type">type</pair>
                            <pair key="uid">uid</pair>
                        </object>
                    </array>
                </pair>
                <pair key="uid">uid</pair>
                <pair key="displayPath">path</pair>
            </object>
        </array>
    </pair>
</object>