<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <class name="com.zutubi.pulse.model.BuildResult" table="BUILD_RESULT" lazy="false">
        <id name="id" type="java.lang.Long" column="ID" unsaved-value="0">
            <generator class="hilo"/>
        </id>

        <many-to-one name="specName" column="SPEC_NAME" class="com.zutubi.pulse.core.model.PersistentName" cascade="none" lazy="false"/>
    </class>

    <class name="com.zutubi.pulse.model.RecipeResultNode" table="RECIPE_RESULT_NODE" lazy="false">
        <id name="id" type="java.lang.Long" column="ID" unsaved-value="0">
            <generator class="hilo"/>
        </id>

        <many-to-one name="stageName" column="STAGE_NAME" class="com.zutubi.pulse.core.model.PersistentName" cascade="none" lazy="false"/>
    </class>
</hibernate-mapping>