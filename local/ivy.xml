<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="http://www.jayasoft.fr/org/ivyrep/ivy-doc.xsl"?>
<ivy-module version="1.0">

    <info organisation="cinnamonbob" module="local" revision="0.3.1" status="release">
        <ivyauthor name="cinnamonbob" url="http://www.cinnamonbob.com/"/>
        <description homepage="http://www.cinnamonbob.com/">
            description....
        </description>
    </info>

    <configurations>
        <conf name="external" description="external dependencies"/>
        <conf name="release"/>
        <conf name="test"/>
    </configurations>

    <publications>
        <artifact name="local" type="jar" conf="release"/>
    </publications>

    <dependencies>

        <!-- command line parsing -->
        <dependency org="apache" name="commons-cli" rev="1.0" conf="external->default"/>

        <dependency org="cinnamonbob" name="core" rev="0.3.1" conf="test->test"/>
        <dependency org="cinnamonbob" name="core" rev="0.3.1" conf="release->release"/>
        <dependency org="cinnamonbob" name="core" rev="0.3.1" conf="external->external"/>

    </dependencies>


</ivy-module>