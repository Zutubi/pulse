<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="http://www.jayasoft.fr/org/ivyrep/ivy-doc.xsl"?>
<ivy-module version="1.0">

    <info organisation="opensymphony" revision="2.2.1" module="sitemesh" status="release">
        <license name="Apache License, Version 2.0" url="http://www.apache.org/licenses/LICENSE-2.0.txt"/>
        <description homepage="http://www.opensymphony.com/sitemesh//">
            SiteMesh is a web-page layout and decoration framework and web-application
            integration framework to aid in creating large sites consisting of many
            pages for which a consistent look/feel, navigation and layout scheme
            is required.
        </description>
    </info>

    <publications>
        <artifact name="sitemesh" type="jar" conf="default"/>
        <artifact name="sitemesh-license" type="txt" conf="default"/>
    </publications>

    <dependencies>
        <dependency org="apache" name="commons-collections" rev="3.1"/>
        <dependency org="apache" name="velocity" rev="1.3.1"/>
        <dependency org="apache" name="velocity-tools" rev="1.1"/>
    </dependencies>

</ivy-module>
