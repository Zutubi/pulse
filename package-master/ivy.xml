<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="http://www.jayasoft.fr/org/ivyrep/ivy-doc.xsl"?>
<ivy-module version="1.0">

    <info organisation="zutubi" module="pulse-master-package" status="integration">
        <ivyauthor name="zutubi" url="http://www.zutubi.com/"/>
        <description homepage="http://www.zutubi.com/">
        </description>
    </info>

    <configurations>
        <conf name="build" description=""/>

        <conf name="internal" description=""/>
        <conf name="prepackaged" description=""/>

        <conf name="test" description=""/>
    </configurations>

    <publications>
        <artifact name="local" type="jar" conf="release,build"/>
    </publications>

    <dependencies>

        <dependency org="zutubi" name="pulse-master" rev="latest.integration" conf="build->release"/>

        <dependency org="zutubi" name="pulse-master" rev="latest.integration" conf="internal->bundles"/>
        <dependency org="zutubi" name="core-bundle" rev="latest.integration" conf="internal->package"/>

        <dependency org="zutubi" name="command-ant" rev="latest.integration" conf="prepackaged->package"/>
        <dependency org="zutubi" name="command-make" rev="latest.integration" conf="prepackaged->package"/>
        <dependency org="zutubi" name="command-maven" rev="latest.integration" conf="prepackaged->package"/>
        <dependency org="zutubi" name="command-maven2" rev="latest.integration" conf="prepackaged->package"/>
        <dependency org="zutubi" name="command-xcode" rev="latest.integration" conf="prepackaged->package"/>
        <dependency org="zutubi" name="postprocessor-ocunit" rev="latest.integration" conf="prepackaged->package"/>
        <dependency org="zutubi" name="postprocessor-cppunit" rev="latest.integration" conf="prepackaged->package"/>

        <dependency org="jwebunit" name="jwebunit" rev="1.2" conf="test->default"/>
        <dependency org="zutubi" name="pulse-master" rev="latest.integration" conf="test->build"/>

    </dependencies>


</ivy-module>