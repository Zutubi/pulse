<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>

    <bean id="startupManager" class="com.zutubi.pulse.bootstrap.DefaultStartupManager">
        <property name="systemContexts">
            <list>
                <value>com/zutubi/pulse/bootstrap/applicationContext.xml</value>
            </list>
        </property>
        <property name="startupRunnables">
            <list>
                <value>com.zutubi.pulse.ResourceDiscoverer</value>
            </list>
        </property>
        <property name="objectFactory">
            <ref local="objectFactory"/>
        </property>
    </bean>

    <bean id="paths" class="com.zutubi.pulse.slave.DefaultSlavePaths"/>
    
    <bean id="objectFactory" class="com.zutubi.pulse.spring.SpringObjectFactory"/>

</beans>
