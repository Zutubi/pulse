<html>
<head>
    <title>create-project</title>
</head>
<content tag="selectedTab">create-project</content>
<body>

    #parse("/template/includes/actionerrors.vm")
    
#bodytag (Form "action='createProject.action'" "method='post'")  
    
    <fieldset>
        #tag (TextField "theme='css'" "name='project.name'")
        #tag (TextField "theme='css'" "name='project.description'")        
    </fieldset>
        
    <fieldset class="submit">
        #tag (Submit "value=getText('cancel.name')" "theme='css'" "name='cancel'")
        #tag (Submit "value=getText('save.name')" "theme='css'" "name='save'")
    </fieldset>

#end 

</body>
</html>
