<html>
<head>
    <title>create-scm</title>
</head>
<content tag="selectedTab">create-scm</content>
<body>

    #parse("/template/includes/actionerrors.vm")

#bodytag (Form "action='createSvn.action'" "method='post'")

    #tag (Hidden "name='id'")

    <fieldset>
        #tag (TextField "required='true'" "name='svn.name'")
        #tag (TextField "required='true'" "name='svn.username'")
        #tag (Password "name='svn.password'")
        #tag (TextField "required='true'" "name='svn.url'")
        #tag (TextField "name='svn.path'")
        #tag (TextField "name='svn.keyfile'")
        #tag (Password "name='svn.passphrase'")
    </fieldset>

    <fieldset class="submit">
        #tag (Submit "value=getText('cancel.name')" "name='cancel'")
        #tag (Submit "value=getText('save.name')" "name='save'")
    </fieldset>

 #end

</body>
</html>
