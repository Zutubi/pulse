<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
<hibernate-mapping>

    <class name="com.cinnamonbob.core2.BuildResult" table="BuildResult">

        <id name="id" type="java.lang.Long" column="ID" unsaved-value="0">
            <generator class="hilo"/>
        </id>

        <property name="succeeded" column="SUCCEEDED" type="boolean" not-null="false"/>

        <property name="projectName" column="PROJECT_NAME" type="string" not-null="false" length="255"/>

        <property name="stamps" type="com.cinnamonbob.util.TimeStampsType">
            <column name="start" sql-type="BIGINT"/>
            <column name="end"  sql-type="BIGINT"/>
        </property>
        
        <property name="stateName" column="STATE" type="string" length="127"/>
        
    </class>

</hibernate-mapping>