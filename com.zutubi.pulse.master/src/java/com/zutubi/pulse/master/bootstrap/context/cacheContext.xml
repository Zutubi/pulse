<?xml version="1.0" encoding="UTF-8"?>
<!--
The cache subsystem context contains the cache manager and related event configurations.

This context exports:

    cacheManager: the systems cache manager.

-->
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
       default-autowire="byName">

    <bean id="cacheManager" class="com.zutubi.pulse.master.util.cache.ehcache.EhCacheManager" init-method="init"/>
    
</beans>