<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<!--
This subsystem context file contains the systems hibernate mappings.

This context exports:

        hibernateMappings: a list of the hibernate mappings defining the database configuration.

-->
<beans>

    <bean id="hibernateMappings" class="com.zutubi.pulse.servercore.bootstrap.SpringListBean">
        <property name="list">
            <list>
                <value>com/zutubi/pulse/master/model/persistence/hibernate/BuildResult.hbm.xml</value>
                <value>com/zutubi/pulse/master/model/persistence/hibernate/Changelist.hbm.xml</value>
                <value>com/zutubi/pulse/master/model/persistence/hibernate/CommandResult.hbm.xml</value>
                <value>com/zutubi/pulse/master/model/persistence/hibernate/Comment.hbm.xml</value>
                <value>com/zutubi/pulse/master/model/persistence/hibernate/Feature.hbm.xml</value>
                <value>com/zutubi/pulse/master/model/persistence/hibernate/HostState.hbm.xml</value>
                <value>com/zutubi/pulse/master/model/persistence/hibernate/Project.hbm.xml</value>
                <value>com/zutubi/pulse/master/model/persistence/hibernate/AgentState.hbm.xml</value>
                <value>com/zutubi/pulse/master/model/persistence/hibernate/AgentSynchronisationMessage.hbm.xml</value>
                <value>com/zutubi/pulse/master/model/persistence/hibernate/AgentDailyStatistics.hbm.xml</value>
                <value>com/zutubi/pulse/master/model/persistence/hibernate/StoredArtifact.hbm.xml</value>
                <value>com/zutubi/pulse/master/model/persistence/hibernate/Sequence.hbm.xml</value>
                <value>com/zutubi/pulse/master/model/persistence/hibernate/TestCaseIndex.hbm.xml</value>
                <value>com/zutubi/pulse/master/model/persistence/hibernate/Trigger.hbm.xml</value>
                <value>com/zutubi/pulse/master/model/persistence/hibernate/User.hbm.xml</value>
            </list>
        </property>
    </bean>

</beans>
