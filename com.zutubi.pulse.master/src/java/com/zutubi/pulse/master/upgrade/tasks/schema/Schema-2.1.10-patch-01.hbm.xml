<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <class name="com.zutubi.pulse.master.model.User" table="LOCAL_USER" lazy="false">
        <id name="id" type="java.lang.Long" column="ID" unsaved-value="0">
            <generator class="hilo"/>
        </id>

        <property name="lastAccessTime" type="long">
            <column name="LAST_ACCESS_TIME" default="0"/>
        </property>
    </class>
</hibernate-mapping>
