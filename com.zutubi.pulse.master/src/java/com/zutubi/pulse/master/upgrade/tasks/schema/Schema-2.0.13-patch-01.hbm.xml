<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>

    <class name="com.zutubi.pulse.master.model.BuildResult" table="BUILD_RESULT" lazy="false">
        <id name="id" type="java.lang.Long" column="ID" unsaved-value="0">
            <generator class="hilo"/>
        </id>

        <property name="revisionString" type="string" length="255">
            <column name="REVISION_STRING" index="idx_build_revision_string"/>
        </property>
    </class>

</hibernate-mapping>