<?xml version="1.0"?>
<project defaultRecipe="xcode build">

    #parse("pulse-file/prologue.vm")

    <recipe name="xcode build">
        <xcodebuild name="build"
#if($workingDir)
                    working-dir="${base.dir}/$workingDir"
#end
#if($config)
                    config="$config"
#end
#if($target)
                    target="$target"
#end
#if($project)
                    project="$project"
#end
#if ($action)
                    buildaction="$action"
#end
#if($settings)
                    settings="$settings"
#end
        >
            #parse("pulse-file/output.processors.vm")
            #parse("pulse-file/artifacts.vm")
        </xcodebuild>
    </recipe>
</project>
