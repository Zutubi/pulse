<?xml version="1.0"?>
<project>
    <regex.pp name="empty"/>

    <regex.pp name="noFailOnError" fail-on-error="false"/>

    <regex.pp name="failOnWarning" fail-on-warning="true"/>

    <regex.pp name="errorPattern">
        <pattern category="error" expression="."/>
    </regex.pp>

    <regex.pp name="warningPattern">
        <pattern category="warning" expression="."/>
    </regex.pp>

    <regex.pp name="infoPattern">
        <pattern category="info" expression="."/>
    </regex.pp>

    <regex.pp name="context" leadingContext="1" trailingContext="2"/>

</project>
