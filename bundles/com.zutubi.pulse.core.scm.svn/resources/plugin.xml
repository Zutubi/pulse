<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
    <extension point="com.zutubi.pulse.core.config">
        <config class="com.zutubi.pulse.core.scm.svn.config.SubversionConfiguration"/>
    </extension>
    <extension point="com.zutubi.pulse.core.scms">
        <scm name="svn"
             factory-class="com.zutubi.pulse.core.scm.svn.SubversionClientFactory"
             working-copy-class="com.zutubi.pulse.core.scm.svn.SubversionWorkingCopy"
             patch-format="standard"/>
    </extension>
</plugin>
