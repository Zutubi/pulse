<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
    <extension point="com.zutubi.pulse.core.commands">
        <command name="xcodebuild" class="com.zutubi.pulse.core.commands.xcode.XCodeCommandConfiguration">
            <resource name="xcode" optional="true"/>
        </command>
    </extension>
    <extension point="com.zutubi.pulse.core.postprocessors">
        <post-processor name="xcode.pp" display-name="xcodebuild output processor"
                        default-processor="true" class="com.zutubi.pulse.core.commands.xcode.XCodePostProcessorConfiguration"/>
    </extension>
</plugin>
