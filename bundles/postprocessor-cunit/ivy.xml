<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="http://www.jayasoft.fr/org/ivyrep/ivy-doc.xsl"?>
<ivy-module version="1.0">
    <info organisation="zutubi" module="com.zutubi.pulse.core.postprocessors.cunit" status="integration">
        <ivyauthor name="Zutubi" url="http://www.zutubi.com/"/>
    </info>

    <configurations>
        <conf name="build"/>
        <conf name="bundle"/>
        <conf name="external"/>
        <conf name="bundled_3rd_party"/>
        <conf name="src" description="The source packages included as part of the source release"/>
    </configurations>

    <publications>
        <artifact name="com.zutubi.pulse.core.postprocessors.cunit" type="jar" conf="bundle,build"/>
        <artifact name="com.zutubi.pulse.core.postprocessors.cunit-src" type="jar" conf="src"/>
        <artifact name="com.zutubi.pulse.core.postprocessors.cunit-test" type="jar" conf="build"/>
    </publications>

    <dependencies>
        <dependency org="zutubi" name="com.zutubi.pulse.core.commands.core" rev="latest.integration" conf="*->@"/>
    </dependencies>
</ivy-module>

